{
  "name": "covid-tracker",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite",
    "server:dev": "nodemon src/server/server.js",
    "server:build": "babel --verbose --out-dir dist --only 'src/server.js,src/lib/' src",
    "client:build": "vite build",
    "type-check": "tsc",
    "lint": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write 'src/**/*.{css,json,md,mdx,js,jsx,ts,tsx}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.10.1",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@headlessui/react": "^1.6.4",
    "antd": "^4.20.7",
    "auth0": "^2.42.0",
    "auth0-js": "^9.19.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "bootstrap": "^5.1.3",
    "concurrently": "^7.2.0",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-jwt": "^7.7.5",
    "express-jwt-permissions": "^1.3.6",
    "express-openid-connect": "^2.7.2",
    "express-session": "^1.17.3",
    "helmet": "^5.1.0",
    "husky": "^8.0.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.3",
    "lint": "^0.7.0",
    "lint-staged": "^12.4.1",
    "nocache": "^3.0.4",
    "nodemon": "^2.0.16",
    "passport": "^0.6.0",
    "passport-auth0": "^1.4.2",
    "react": "^18.0.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^18.1.0",
    "react-query": "^3.39.0",
    "react-router-dom": "^6.3.0",
    "sass": "^1.51.0",
    "vite": "^2.9.9",
    "vite-plugin-node": "^1.0.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "husky": "^8.0.0",
    "prettier": "^2.6.2"
  }
}
